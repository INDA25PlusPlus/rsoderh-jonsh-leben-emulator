;
; Print out the first 12 fibonacci numbers
;

        ORG 0

START:  
        MVI A, 1
        MVI B, 1
        MVI D, 1        ; D ticks up to 12
        MVI E, 12

LOOP:   
        OUT 1           ; print decimal format

        MOV L, A
        MVI A, 20H
        OUT 0
        MOV A, L

        ADD B           ; A += B

        MOV C, A        ; temp store A
        MOV A, D

        CMP E
        JZ STOP         ; if D == 0

        MOV A, B        ; swap A, B
        MOV B, C

        INR D           ; D++
        JMP LOOP

STOP:
        END
